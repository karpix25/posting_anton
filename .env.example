# Yandex Disk Token
# Получить можно через OAuth приложение Яндекс: https://oauth.yandex.ru/
YANDEX_TOKEN=your_yandex_oauth_token

# OpenAI API Key
# Для генерации текстов (GPT-4)
OPENAI_API_KEY=sk-proj-...

# Upload Post API Key
# Для публикации видео и синхронизации профилей
# Получить в сервисе Upload Post
UPLOAD_POST_API_KEY=your_upload_post_api_key

# Postgres Connection (для KPI монтажеров и статистики)
# Пример: postgres://admin:password@host:5432/postgres?sslmode=disable
# Если оставить пустым, будет использоваться тестовое значение
DATABASE_URL=
